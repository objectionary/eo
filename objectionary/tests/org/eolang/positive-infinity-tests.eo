+alias org.eolang.hamcrest.assert-that
+architect yegor256@gmail.com
+home https://github.com/objectionary/eo
+package org.eolang
+junit
+version 0.29.0

[] > positive-infinity-eq-is-valid
  assert-that > @
    and.
      positive-infinity.eq positive-infinity
      (positive-infinity.eq negative-infinity).eq FALSE
      (positive-infinity.eq nan).eq FALSE
      (positive-infinity.eq 42).eq FALSE
      (positive-infinity.eq 42.5).eq FALSE
    $.equal-to TRUE

[] > positive-infinity-neq-is-valid
  assert-that > @
    and.
      (positive-infinity.neq positive-infinity).eq FALSE
      positive-infinity.neq negative-infinity
      positive-infinity.neq nan
      positive-infinity.neq 42
      positive-infinity.neq 42.5
    $.equal-to TRUE

[] > positive-infinity-lt-is-valid
  assert-that > @
    and.
      (positive-infinity.lt positive-infinity).eq FALSE
      (positive-infinity.lt negative-infinity).eq FALSE
      (positive-infinity.lt nan).eq FALSE
      (positive-infinity.lt 42).eq FALSE
      (positive-infinity.lt 42.5).eq FALSE
    $.equal-to TRUE

[] > positive-infinity-lte-is-valid
  assert-that > @
    and.
      positive-infinity.lte positive-infinity
      (positive-infinity.lte negative-infinity).eq FALSE
      (positive-infinity.lte nan).eq FALSE
      (positive-infinity.lte 42).eq FALSE
      (positive-infinity.lte 42.5).eq FALSE
    $.equal-to TRUE

[] > positive-infinity-gt-is-valid
  assert-that > @
    and.
      (positive-infinity.gt positive-infinity).eq FALSE
      positive-infinity.gt negative-infinity
      (positive-infinity.gt nan).eq FALSE
      positive-infinity.gt 42
      positive-infinity.gt 42.5
    $.equal-to TRUE

[] > positive-infinity-gte-is-valid
  assert-that > @
    and.
      positive-infinity.gte positive-infinity
      positive-infinity.gte negative-infinity
      (positive-infinity.gte nan).eq FALSE
      positive-infinity.gte 42
      positive-infinity.gte 42.5
    $.equal-to TRUE

[] > positive-infinity-times-is-valid
  assert-that > @
    and.
      (positive-infinity.times 0.0).as-bytes.eq nan.as-bytes
      (positive-infinity.times 0).as-bytes.eq nan.as-bytes
      (positive-infinity.times nan).as-bytes.eq nan.as-bytes
      (positive-infinity.times negative-infinity).eq negative-infinity
      (positive-infinity.times positive-infinity).eq positive-infinity
      (positive-infinity.times 42).eq positive-infinity
      (positive-infinity.times -42).eq positive-infinity
      (positive-infinity.times 42.5).eq positive-infinity
      (positive-infinity.times -42.5).eq positive-infinity
    $.equal-to TRUE

[] > positive-infinity-plus-is-valid
  assert-that > @
    and.
      (positive-infinity.plus negative-infinity).as-bytes.eq nan.as-bytes
      (positive-infinity.plus nan).as-bytes.eq nan.as-bytes
      (positive-infinity.plus 0.0).eq positive-infinity
      (positive-infinity.plus positive-infinity).eq positive-infinity
      (positive-infinity.plus 42).eq positive-infinity
      (positive-infinity.plus 42.5).eq positive-infinity
    $.equal-to TRUE

[] > positive-infinity-neg-is-negative-infinity
  assert-that > @
    positive-infinity.neg
    $.equal-to negative-infinity

[] > positive-infinity-signum-float-one
  assert-that > @
    positive-infinity.signum
    $.equal-to 1.0

[] > positive-infinity-minus-is-valid
  assert-that > @
    and.
      (positive-infinity.minus positive-infinity).as-bytes.eq nan.as-bytes
      (positive-infinity.minus nan).as-bytes.eq nan.as-bytes
      (positive-infinity.minus 0.0).eq positive-infinity
      (positive-infinity.minus negative-infinity).eq positive-infinity
      (positive-infinity.minus 42).eq positive-infinity
      (positive-infinity.minus 42.5).eq positive-infinity
    $.equal-to TRUE

[] > positive-infinity-div-is-valid
  assert-that > @
    and.
      (positive-infinity.div nan).as-bytes.eq nan.as-bytes
      (positive-infinity.div negative-infinity).as-bytes.eq nan.as-bytes
      (positive-infinity.div positive-infinity).as-bytes.eq nan.as-bytes
      (positive-infinity.div 0.0).eq positive-infinity
      (positive-infinity.div 42).eq positive-infinity
      (positive-infinity.div -42).eq positive-infinity
      (positive-infinity.div 42.5).eq positive-infinity
      (positive-infinity.div -42.5).eq positive-infinity
    $.equal-to TRUE

[] > positive-infinity-as-bytes-is-valid
  assert-that > @
    positive-infinity.as-bytes
    $.equal-to 7F-80-00-00

[] > cannot-convert-positive-infinity-to-int
  assert-that > @
    try
      []
        positive-infinity.as-int > @
      [e]
        e > @
      nop
    $.equal-to "You can not convert positive infinity to int"

[] > positive-infinity-as-string-is-valid
  assert-that > @
    positive-infinity.as-string
    $.equal-to "+âˆž"
