[] > rust-bind-to-copy
  # Book.
  [content] > book
  "qwerty" > line
  QQ.rust > applied
    """
    use eo::Portal;
    use eo::eo_enum::EO;
    use eo::eo_enum::EO::{EOVertex};

    pub fn foo(portal: &mut Portal) -> Option<EO> {
      let eobook = portal.find("$.^.book")?;
      let copy = portal.copy(eobook)?;
      let eoline = portal.find("$.^.line")?;
      portal.bind(copy.clone(), eoline, "content")?;
      Some(EOVertex(copy))
    }
    """
    [] >>
    *
  eq. > @
    applied.content
    "qwerty"

