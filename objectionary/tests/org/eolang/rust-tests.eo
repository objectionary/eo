[] > rust-put-to-copy
  QQ.rust > data
    """
    use eo_env::EOEnv;
    use eo_env::eo_enum::EO;
    use eo_env::eo_enum::EO::{EOVertex, EOError};

    pub fn foo(env: &mut EOEnv) -> Option<EO> {
