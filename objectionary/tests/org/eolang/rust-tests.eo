    """
    use eo::Portal;
    use eo::eo_enum::EO;
    use eo::eo_enum::EO::{EOVertex};

    pub fn foo(portal: &mut Portal) -> Option<EO> {
      let eobook = portal.find("$.^.book")?;
      let copy = portal.copy(eobook)?;
      let eoline = portal.find("$.^.line")?;
