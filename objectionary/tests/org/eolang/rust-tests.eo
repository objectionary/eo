[] > rust-bind-to-copy
  [content] > book
  "qwerty" > line
  QQ.rust > applied
    """
    use eo_env::EOEnv;
    use eo_env::eo_enum::EO;
    use eo_env::eo_enum::EO::{EOVertex};

    pub fn foo(env: &mut EOEnv) -> Option<EO> {
      let eobook = env.find("$.^.book")?;
      let copy = env.copy(eobook)?;
      let eoline = env.find("$.^.line")?;
      env.bind(copy.clone(), eoline, "content")?;
      Some(EOVertex(copy))
    }
    """
    *
      []
  eq. > @
    applied.content
    "qwerty"
