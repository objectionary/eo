# Test.
[] > takes-part-of-bytes
  eq. > @
    slice.
      20-1F-EE-B5-90
      1
      3
    1F-EE-B5

# Test.
[] > size-of-part-is-correct
  eq. > @
    size.
      slice.
        20-1F-EE-B5-90-EE-BB
        2
        3
    3

# Test.
[] > counts-size-of-bytes
  eq. > @
    size.
      F1-20-5F-EC-B5-90-32
    7

