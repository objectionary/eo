+package org.eolang
+rt jvm org.eolang:eo-runtime:0.16.0

[] > bool
  # Equal to another bool?
  [b] > eq /bool

  # If true, return "t", otherwise "f"
  [t f] > if /?

  # NOT
  [] > not /bool

  # AND
  [x...] > and /bool

  # OR
  [x...] > or /bool

  # While this is true copies the object
  # with "i" as the position of the cycle. The result
  # of WHILE is the number of successful cycles.
  [f] > while /int
