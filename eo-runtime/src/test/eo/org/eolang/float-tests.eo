+alias org.eolang.hamcrest.assert-that
+architect yegor256@gmail.com
+home https://github.com/objectionary/eo
+package org.eolang
+junit
+version 0.0.0

[] > less-true
  assert-that > @
    -10.5
    $.less-than 50.3

[] > less-equal
  assert-that > @
    -10.7
    $.not
      $.less-than -10.7

[] > less-false
  assert-that > @
    10.13
    $.not
      $.less-than -5.3

[] > greater-true
  assert-that > @
    -200.1
    $.greater-than -1000.0

[] > greater-false
  assert-that > @
    0.5
    $.not
      $.greater-than 100.1

[] > greater-equal
  assert-that > @
    0.7
    $.not
      $.greater-than 0.7

[] > leq-true
  assert-that > @
    lte.
      -200.5
      -100.6
    $.equal-to TRUE

[] > leq-equal
  assert-that > @
    lte.
      50.1
      50.1
    $.equal-to TRUE

[] > leq-false
  assert-that > @
    lte.
      0.9
      -10.11
    $.equal-to FALSE

[] > gte-true
  assert-that > @
    gte.
      -1000.1
      -1100.1
    $.equal-to TRUE

[] > gte-equal
  assert-that > @
    gte.
      113.333
      113.333
    $.equal-to TRUE

[] > gte-false
  assert-that > @
    gte.
      0.7
      10.9
    $.equal-to FALSE

[] > float-zero-not-equal-to-nan
  assert-that > @
    0.0
    $.not
      $.equal-to
        nan

[] > float-not-equal-to-nan
  assert-that > @
    52.4
    $.not
      $.equal-to
        nan

[] > float-not-equal-to-positive-infinity
  assert-that > @
    52.4
    $.not
      $.equal-to
        positive-infinity

[] > float-not-equal-to-negative-infinity
  assert-that > @
    52.4
    $.not
      $.equal-to
        negative-infinity

[] > zero-eq-to-zero
  assert-that > @
    eq.
      0.0
      0.0
    $.equal-to TRUE

[] > zero-not-equal-to-neg-zero
  assert-that > @
    0.0
    $.not
      $.equal-to -0.0

[] > neg-zero-not-equal-to-zero
  assert-that > @
    -0.0
    $.not
      $.equal-to 0.0

[] > zero-not-greater-that-neg-zero
  assert-that > @
    0.0
    $.not
      $.greater-than -0.0

[] > neg-zero-not-greater-that-zero
  assert-that > @
    -0.0
    $.not
      $.greater-than 0.0

[] > zero-not-less-that-neg-zero
  assert-that > @
    0.0
    $.not
      $.less-than -0.0

[] > neg-zero-not-less-that-zero
  assert-that > @
    -0.0
    $.not
      $.less-than 0.0

[] > zero-not-gte-neg-zero
  assert-that > @
    0.0.gte -0.0
    $.equal-to FALSE

[] > neg-zero-not-gte-zero
  assert-that > @
    -0.0.gte 0.0
    $.equal-to FALSE

[] > zero-not-lte-neg-zero
  assert-that > @
    0.0.lte -0.0
    $.equal-to FALSE

[] > neg-zero-not-lte-zero
  assert-that > @
    -0.0.lte 0.0
    $.equal-to FALSE

[] > float-zero-not-eq-to-int-zero
  assert-that > @
    eq.
      0.0
      0
    $.equal-to FALSE

[] > eq-true
  assert-that > @
    123.5
    $.equal-to 123.5

[] > eq-false
  assert-that > @
    123.5
    $.not
      $.equal-to 42.4

[] > compares-two-different-types
  assert-that > @
    3.14
    $.not
      $.equal-to "Hello"

[] > times-by-zero
  assert-that > @
    times.
      574.9
      0.0
    $.equal-to 0.0

[] > times-by-one
  assert-that > @
    times.
      574.9
      1.0
    $.equal-to 574.9

[] > times-two-positives
  assert-that > @
    times.
      574.5
      311.985
    $.equal-to 179235.3825

[] > times-two-negatives
  assert-that > @
    times.
      -574.5
      -311.985
    $.equal-to 179235.3825

[] > times-diff-sign
  assert-that > @
    times.
      -0.5745
      0.311985
    $.equal-to -0.1792353825

[] > times-multiple
  assert-that > @
    times.
      -0.5745
      0.311985
      3.232
      -2.532
    $.equal-to 1.4667591307996803

[] > add-test
  assert-that > @
    plus.
      574.5
      311.985
    $.equal-to 886.485

[] > add-multiple-test
  assert-that > @
    plus.
      574.5
      311.985
      -101.1
      1.001
    $.equal-to 786.386

[] > add-wrong-args
  assert-that > @
    try
      []
        plus. > @
          574.5
          311
          -101.1
          1.001
      [e]
        e > @
      nop
    $.equal-to "The 1th argument of 'float.plus' is not a(n) Double: 311"

[] > neg-test
  assert-that > @
    886.485.neg
    $.equal-to -886.485

[] > sub-test
  assert-that > @
    minus.
      574.5
      311.985
    $.equal-to 262.515

[] > sub-multiple-test
  assert-that > @
    minus.
      574.5
      311.985
      -24.347
      3542.2238
    $.equal-to -3255.3618

[] > div-test
  assert-that > @
    div.
      15.5
      0.5
    $.equal-to 31.0

[] > div-multiple
  assert-that > @
    div.
      15.5
      0.5
      0.2
    $.equal-to 155.0

[] > zero-div-zero-as-bytes-is-equal-to-nan-as-bytes
  assert-that > @
    (0.0.div 0.0).as-bytes
    $.equal-to nan.as-bytes

[] > float-times-nan-is-nan
  nan > not-a-number!
  assert-that > @
    as-bytes.
      35.2.times not-a-number
    $.equal-to not-a-number.as-bytes

[] > float-plus-nan-is-nan
  nan > not-a-number!
  assert-that > @
    as-bytes.
      35.2.plus not-a-number
    $.equal-to not-a-number.as-bytes

[] > float-minus-nan-is-nan
  nan > not-a-number!
  assert-that > @
    as-bytes.
      35.2.minus not-a-number
    $.equal-to not-a-number.as-bytes

[] > float-div-nan-is-nan
  nan > not-a-number!
  assert-that > @
    as-bytes.
      35.2.div not-a-number
    $.equal-to not-a-number.as-bytes

[] > float-not-gt-nan
  assert-that > @
    52.3
    $.not
      $.greater-than nan

[] > float-not-gte-nan
  assert-that > @
    52.3.gte nan
    $.equal-to FALSE

[] > float-not-lt-nan
  assert-that > @
    52.3
    $.not
      $.less-than nan

[] > float-not-lte-nan
  assert-that > @
    52.3.lte nan
    $.equal-to FALSE

[] > one-div-zero-is-positive-infinity
  assert-that > @
    1.0.div 0.0
    $.equal-to positive-infinity

[] > minus-one-div-zero-is-negative-infinity
  assert-that > @
    -1.0.div 0.0
    @.equal-to negative-infinity

[] > positive-float-plus-positive-infinity
  positive-infinity > pos-inf!
  assert-that > @
    42.5.plus pos-inf
    $.equal-to pos-inf

[] > positive-float-minus-positive-infinity
  assert-that > @
    42.5.minus positive-infinity
    $.equal-to negative-infinity

[] > positive-float-times-positive-infinity
  positive-infinity > pos-inf!
  assert-that > @
    42.5.times pos-inf
    $.equal-to pos-inf

[] > positive-float-div-positive-infinity
  assert-that > @
    42.5.div positive-infinity
    $.equal-to 0.0

[] > negative-float-plus-positive-infinity
  positive-infinity > pos-inf!
  assert-that > @
    -42.5.plus pos-inf
    $.equal-to pos-inf

[] > negative-float-minus-positive-infinity
  assert-that > @
    -42.5.minus positive-infinity
    $.equal-to negative-infinity

[] > negative-float-times-positive-infinity
  assert-that > @
    -42.5.times positive-infinity
    $.equal-to negative-infinity

[] > negative-float-div-positive-infinity
  assert-that > @
    -42.5.div positive-infinity
    $.equal-to -0.0

[] > positive-float-plus-negative-infinity
  negative-infinity > neg-inf!
  assert-that > @
    42.5.plus neg-inf
    $.equal-to neg-inf

[] > positive-float-minus-negative-infinity
  assert-that > @
    42.5.minus negative-infinity
    $.equal-to positive-infinity

[] > positive-float-times-negative-infinity
  negative-infinity > neg-inf!
  assert-that > @
    42.5.times neg-inf
    $.equal-to neg-inf

[] > positive-float-div-negative-infinity
  assert-that > @
    42.5.div negative-infinity
    $.equal-to -0.0

[] > negative-float-plus-negative-infinity
  negative-infinity > neg-inf!
  assert-that > @
    -42.5.plus neg-inf
    $.equal-to neg-inf

[] > negative-float-minus-negative-infinity
  assert-that > @
    -42.5.minus negative-infinity
    $.equal-to positive-infinity

[] > negative-float-times-negative-infinity
  assert-that > @
    -42.5.times negative-infinity
    $.equal-to positive-infinity

[] > negative-float-div-negative-infinity
  assert-that > @
    -42.5.div negative-infinity
    $.equal-to 0.0

[] > to-bytes-and-backwards
  assert-that > @
    as-float.
      as-bytes.
        3.1415926
    $.equal-to 3.1415926

[] > as-bytes-equals-to-float
  assert-that > @
    3.1415926.as-bytes
    $.equal-to 3.1415926

[] > as-bytes-equals-to-float-backwards
  assert-that > @
    3.1415926.as-bytes
    $.equal-to 3.1415926

[] > similar-values-have-similar-hash
  assert-that > @
    (3.1415926).as-hash
    $.equal-to
      (3.1415926).as-hash

[] > inline-float-computation
  ((4.0).plus (5.7)).eq (9.7) > @
