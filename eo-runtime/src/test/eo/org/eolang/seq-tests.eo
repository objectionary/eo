# Test.
[] > seq-single-dataization-float-less
  malloc.of > @
    1
    [b]
      malloc.for > @
        0.0
        [m] >>
          b.put > @
            lt.
              seq
                *
                  m.put (m.as-number.plus 1.0)
                  m.as-number
              1.1

