          42
          1

  # Tests that the runtime constants defend against side effects.
  [] +> tests-constant-defends-against-side-effects
    eq. > @
      malloc.for 7 [m]>>
        m.put > @
          times.
            num
            num
        number > num
          ^.inc m > n!
      64
    [x] > inc
      seq > @
        *
          x.put
            x.as-number.plus 1
          x.as-number

