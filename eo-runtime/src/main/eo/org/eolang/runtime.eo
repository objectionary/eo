# Runtime.
#
# The runtime object provides a collection of test cases that verify
# core EO language functionality and behavior.
#
# @todo #4235:35min Move all tests from `runtime.eo` to other objects.
#  For now runtime is just a set of runtime tests. Instead of having this, let's move its tests
#  to the other objects, which tests are most suitable for.
[] > runtime
  # This unit test is supposed to check the globals bound objects work as tests.
  true +> global-test

  # Tests that the runtime understands 'this' reference correctly.
  [] +> tests-understands-this-correctly
    eq. > @
      a 42
      42
    [x] > a
      x > @

  # Tests that the runtime takes parent objects correctly.
  [] +> tests-takes-parent-object
    eq. > @
      a 42
      42
    [x] > a
      take > @
      [] > take
        ^.x > @

  # Tests that the runtime makes objects into constants correctly.
  [] +> tests-makes-object-a-constant
    eq. > @
      f
      f
    [] > foo
      times. > @
        50
        50
    foo.@ > f!

  # Tests that the runtime takes parent through attribute access.
  [] +> tests-takes-parent-through-attribute
    [] > @
      [] > @
        [] > @
          eq. > @
            x
            42
    42 > x

  # Tests that the runtime throws an error when applying to closed objects.
  [] +> throws-when-applies-to-closed-object
    closed true > @
    [x] > a
      x > @
    a false > closed

  # Tests that the runtime makes deep objects recursively.
  [] +> tests-makes-deep-object-recursively
    eq. > @
      x 5
      0
    [i] > x
      if. > @
        i.lt 0
        0
        x
          i.minus 1

  # Tests that the runtime calculates expressions only once.
  [] +> tests-calculates-only-once
    eq. > @
      malloc.for
        0
        [m] > x
          seq > @
            *
              a.neg.neg.neg.neg.eq 42
              m
          [] > a
            seq > @
              *
                ^.m.put (^.m.as-number.plus 1)
                42
      1

  # Tests that the runtime handles recursion without arguments.
  [] +> tests-recursion-without-arguments
    eq. > @
      malloc.for 4 [m]>>
        func m > @
      0
    [n] > func
      if. > @
        n.as-number.gt 0
        seq
          *
            n.put (n.as-number.minus 1)
            ^.func n
        n

  # Tests that the runtime unescapes slashes in strings.
  [] +> tests-unescapes-slashes
    eq. > @
      "x\\b\\f\\u\\r\\t\\n\\'"
      78-5C-62-5C-66-5C-75-5C-72-5C-74-5C-6E-5C-27

  # Tests that the runtime unescapes symbols in strings.
  [] +> tests-unescapes-symbols
    eq. > @
      "\b\f\n\r\t\u27E6"
      08-0C-0A-0D-09-E2-9F-A6

  # Tests that the runtime compiles correctly with long duplicate names.
  [] +> tests-compiles-correctly-with-long-duplicate-names
    true > @
    [] > long-object-name
      [] > long-object-name
        [] > long-object-name
          [] > long-object-name
            [] > long-object-name
              42 > x

  # Tests that the runtime handles named inner abstract objects.
  [] +> tests-named-inner-abstract-object
    seq > @
      *
        [] > b
          false > @
        [] > a
          true > @

  # Tests that the runtime handles applications that call functions.
  [] +> tests-app-that-calls-func
    eq. > @
      output
      2
    [] > app
      f > @
        * 1 2 3
      [args] > f
        2 > @
        1 > a
    app > output

  # Tests that the runtime directly accesses objects from root namespace.
  [] +> tests-directly-accesses-objects-from-root
    eq. > @
      Q.org.eolang.malloc.of
        8
        [m]
          seq > @
            *
              m.put 42
              m.put
                m.as-number.minus 2
      40

  # Tests that the runtime directly accesses objects from standard root namespace.
  [] +> tests-directly-accesses-objects-from-standard-root
    eq. > @
      QQ.malloc.of
        8
        [m]
          seq > @
            *
              m.put 42
              m.put
                m.as-number.minus 2
      40

