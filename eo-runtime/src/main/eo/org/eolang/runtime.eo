  # Tests that the runtime correctly handles same name attributes in simplified form.
  [] +> tests-correctly-handles-same-name-attrs-simplified
    eq. > @
      calc
        []
          build 1 > @
          [x] > build
            x > @
        []
          build 2 > @
          [y] > build
            y > @
      3
    [first second] > calc
      plus. > @
        first
        second

  # Tests that the runtime correctly handles same name attributes.
  [] +> tests-correctly-handles-same-name-attrs
    eq. > @
      calc
        []
          build 1 > @
          [x] > build
            x.plus 1 > @
            ^.build > next
              plus.
                x
                1
        []
          build 2 > @
          [y] > build
            y.plus 2 > @
            ^.build > next
              plus.
                y
                2
      9
    [f s] > calc
      plus. > @
        f.next
        s.next

