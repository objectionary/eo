  # Tests that the runtime uses object hierarchy correctly.
  [] +> tests-right-way-to-use-hierarchy
    ((pyint 1).add (pyint 3)).eq (pyint 4) > @
    [value] > pybool
      value > @
    [value] > pyint
      [x] > eq
        ^.^.pybool (^.value.eq x.value) > @
      [x] > add
        ^.^.pyint (^.value.plus x.value) > @

  # Tests that the runtime checks triple quoted strings correctly.
