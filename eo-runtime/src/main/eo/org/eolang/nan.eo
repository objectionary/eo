  # Tests that $ = x
  [x] > eq
    FALSE > @

  # Tests that $ < x
  [x] > lt
    FALSE > @

  # Tests that $ â‰¤ x
  [x] > lte
    FALSE > @

  # Tests that $ > x
  [x] > gt
    FALSE > @

  # Tests that $ â‰¥ x
  [x] > gte
    FALSE > @

  # Multiplication of $ and x
  [x...] > times
    ^ > @

  # Sum of $ and x
  [x...] > plus
    ^ > @

  # Negation of $
  [] > neg
    ^ > @

  # Difference between $ and x
  [x...] > minus
    ^ > @

  # Quotient of the division of $ by x
  [x...] > div
    ^ > @
