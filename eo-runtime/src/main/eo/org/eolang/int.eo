+architect yegor256@gmail.com
+home https://github.com/objectionary/eo
+package org.eolang
+rt jvm org.eolang:eo-runtime:0.0.0
+version 0.0.0

# Integer.
[as-bytes] > int
  as-bytes > @

  # Tests that $ = x.
  [x] > eq
    eq. > @
      ^.as-bytes
      x.as-bytes

  # Tests that $ < x.
  [x] > lt
    gt. > @
      plus.
        x
        neg.
          ^
      0

  # Tests that $ â‰¤ x.
  [x] > lte
    not. > @
      ^.gt x

  # Tests that $ > x.
  [x] > gt /bool

  # Tests that $ â‰¥ x.
  [x] > gte
    not. > @
      ^.lt x

  # Change the sign of the number.
  [] > neg
    ^.times -1 > @

  # Add to the current one.
  [x] > plus /int

  # Subtract from the current one.
  [x] > minus
    ^.plus x.neg > @

  # Multiplication this one by another int.
  [x] > times /int

  # Divide this one by another int.
  [x] > div /int
