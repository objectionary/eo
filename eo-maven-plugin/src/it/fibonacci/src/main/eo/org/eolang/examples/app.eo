+package org.eolang.examples
+alias org.eolang.examples.fibonacci
+alias org.eolang.io.stdout
+alias org.eolang.tt.sprintf
+alias org.eolang.tt.sscanf
+architect yegor256@gmail.com

# Application.
[args] > app
  number > n
    at. > nn!
      QQ.tt.sscanf
        "%d"
        args.at 0
      0
  at. > e!
    QQ.tt.sscanf
      "%d"
      args.at 1
    0
  fibonacci n > f!
  and. > @
    stdout
      sprintf
        "%dth Fibonacci number is %d\n"
        * n f
    e.eq f
