+package sandbox
+alias fibonacci sandbox.fibonacci
+alias stdout org.eolang.io.stdout
+alias sprintf org.eolang.txt.sprintf


[n] > factorial
  [n acc] > subfFactorial
    if. > @
      less.
        n
        2
      acc
      subfFactorial
        sub.
          n
          1
        mul.
          acc
          n
  subfFactorial > @
    n
    1

[args...] > app
  sprintf > @
    "%d! = %d\n"
    (args.get 0).toInt > n
    factorial n
